package sdbms;

public class Student {

	private String id;
	private int age;
	private String name;
	//private int marks;
	private double numSubject;
	
	private static int count=101;

	public Student( int age, String name, double numSubject) {
		this.id="JSP"+count;
		count++;
		this.age=age;
		this.name=name;
		this.numSubject=numSubject;
		
	}


	public String getId() {
		return id;
	}

	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age=age;
	}

	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name=name;
	}

	public double getNumSubjects() {
		return numSubject;
	}
	public void setMarks(double numSubject) {
		this.numSubject=numSubject;
	}
	





	public String toString() {
		return"ID:"+id+" Age:"+age+" Name:"+name+" numSubjects:"+numSubjects;
	}
	
	
	
}
