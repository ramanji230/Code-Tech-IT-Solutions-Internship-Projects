package codeTech_IT_Solutions.com;

import tic_tac_toe_Game.codeTech.TicTacToe;

public class Player {

	String name;
	char mark;
	
	
	 void makeMove();

	boolean isVaildMove( int row, int col) {
		if(row>=0 && row <=2 && 
				col >=0 && col <=2) {
			if(TicTacToe.board[row][col] == ' ') {
				return true;
			}

		}
		return false;
	}
}

}
